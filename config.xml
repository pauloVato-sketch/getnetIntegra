<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget android-versionCode="60804" id="com.odhen.POS" version="6.0.8.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>odhenPOS</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <plugin name="cordova-plugin-keyboard" spec="^1.2.0"/>
  <plugin name="cordova-plugin-android-permissions" spec="^1.0.0"/>
  <plugin name="phonegap-plugin-barcodescanner" spec="^8.1.0">
    <variable name="ANDROID_SUPPORT_V4_VERSION" value="28.0.0"/>
  </plugin>
  <plugin name="cordova-plugin-device" spec="^2.0.3"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <platform name="android">
    <icon density="mdpi" src="res/icon/android/mdpi.png"/>
    <icon density="ldpi" src="res/icon/android/ldpi.png"/>
    <icon density="hdpi" src="res/icon/android/hdpi.png"/>
    <icon density="xhdpi" src="res/icon/android/xhdpi.png"/>
  </platform>
  <engine name="android" spec="~8.0.0"/>
</widget>