{
	"name": "authContainer",
	"events": [],
	"showHeader": false,
	"showMenu": false,
	"showFooter": false,
	"showBack": false,
	"showHomeButton": true,
	"showNotifications": true,
	"footer": "component/footer.html",
	"template": "container/window.html",
	"popup": "component/popup.html",
	"widgets": [
		{
			"name": "authWidget",
			"isVisible": true,
			"template": "widget/form-login.html",
			"actions": [],
			"events": [
				{
					"name": "WidgetOnEnter",
					"code": " UtilitiesService.changeIndex(args.owner); UtilitiesService.setVersionLabel(args.owner); args.owner.reload(); UtilitiesService.loginOnEnter(args);"
				}
			],
			"dataSource": {
				"rest": false,
				"memoryStorage": true,
				"lazyLoad": true,
				"name": "/nothing"
			},
			"fields": [
				{
					"class": "12 login-logo-img",
					"name": "logo-waiter",
					"label": "logo-waiter",
					"isVisible": true,
					"readOnly": true,
					"source": "/images/logos/odhen_pos.png",
					"sourceWaiter": "/images/logos/odhen_pos.png",
					"sourceFastPass": "/images/logos/fastpass.png",
					"template": "field/image-static.html",
					"events": []
				},
				{
					"class": 12,
					"name": "EMAIL",
					"isVisible": true,
					"readOnly": false,
					"defaultValue": null,
					"label": "Email",
					"template": "field/text.html",
					"events": [
						{
							"name": "FieldOnInput",
							"code": "AuthController.setAuthLogin(args.owner.field.widget);"
						}
					]
				},
				{
					"class": 12,
					"name": "PASSWORD",
					"isVisible": true,
					"readOnly": true,
					"defaultValue": null,
					"label": "Senha",
					"template": "field/password.html",
					"events": []
				},
				{
					"class": 12,
					"name": "AUTH",
					"label": "Entrar",
					"isVisible": true,
					"readOnly": true,
					"defaultValue": null,
					"template": "field/button.html",
					"defaultButton": true,
					"events": [
						{
							"name": "FieldOnClick",
							"code": "AuthController.auth(args.owner.field.widget);"
						}
					]
				},
				{
					"class": 12,
					"name": "version",
					"label": "Vers√£o x.x.x",
					"isVisible": true,
					"readOnly": true,
					"defaultValue": null,
					"template": "field/label-static.html",
					"events": []
				},
				{
					"class": 12,
					"name": "blank",
					"label": "",
					"isVisible": true,
					"readOnly": true,
					"defaultValue": null,
					"template": "field/label-static.html",
					"events": []
				},
				{
					"class": 12,
					"name": "poynt_email",
					"label": "E-mail: contato@teknisa.com",
					"isVisible": false,
					"readOnly": true,
					"defaultValue": null,
					"template": "field/label-static.html",
					"events": []
				},
				{
					"class": 12,
					"name": "poynt_phone",
					"label": "Telefone: +55 31 2122-2300",
					"isVisible": false,
					"readOnly": true,
					"defaultValue": null,
					"template": "field/label-static.html",
					"events": []
				},
				{
					"class": 12,
					"name": "poynt_website",
					"label": "Site: www.teknisa.com",
					"isVisible": false,
					"readOnly": true,
					"defaultValue": null,
					"template": "field/label-static.html",
					"events": []
				}
			]
		}
	]
}
