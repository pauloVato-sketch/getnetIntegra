{
  "tags": {
    "allowUnknownTags": false
  },
  "recurseDepth": 10,
  "source": {
      "include": "src",
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "readme": "README.md",
    "destination": "docs",
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
